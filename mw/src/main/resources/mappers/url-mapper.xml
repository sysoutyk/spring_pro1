<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="mappers.url">
	
	<!-- UserVo -->
	<select id="getUserVo_byUrl" resultType="UserVo">
		select * from tbl_user
		where url = #{url}
	</select>
	
</mapper>